expr = sum;
sum = product sumRest;
sumRest = sumOp product sumRest | '';
sumOp = '+' | '-';
product = value productRest;
productRest = productOp value productRest | '';
productOp = '*' | '/';
value = number | '(' expr ')';
number = nonzero digits;
digits = digit digits | '';
digit = '0' | nonzero;
nonzero = '1' | '2' | '3' | '4' | '5' | '6' | '7' | '8' | '9';